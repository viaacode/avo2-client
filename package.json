{
  "name": "avo2-client",
  "version": "1.33.39",
  "private": true,
  "type": "module",
  "scripts": {
    "--prepare": "husky install",
    "start-with-env": "npm run build && dotenv -e .env.qas node ./scripts/env.js && dotenv -e .env.qas cross-env NODE_ENV=production node ./dist/server/server.js",
    "predev": "dotenv -e .env.qas node ./scripts/env.js && cp env-config.js ./public",
    "dev": "dotenv -e .env.qas node server.ts",
    "dev:embed": "dotenv -e .env.qas vite --config vite-embed.config.mts",
    "build:clean": "shx rm -rf ./dist && shx rm -rf ./dist-embed",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr server.ts",
    "build:embed-build": "vite build --config vite-embed.config.mts",
    "build:embed-copy": "shx mkdir -p ./dist/client/embed && cp ./dist-embed/embed.html ./dist/client/embed/index.html && cp -rf ./dist-embed/assets/* ./dist/client/assets/ && cp ./embed/close-browser.html ./dist/client/embed/close-browser.html",
    "build": "npm run build:client && npm run build:server && npm run build:embed-build && npm run build:embed-copy",
    "add-cookiebot-attribute": "npx ts-node --transpile-only --project ./scripts/tsconfig.json -- scripts/add-cookiebot-ignore-attributes.ts",
    "unit-tests": "jest --transformIgnorePatterns \"node_modules/(?!(lodash-es))/\"",
    "lint": "biome lint",
    "lint:fix": "biome lint --write",
    "format": "biome format --write",
    "type-check": "tsc --noEmit",
    "whitelist-queries": "npx ts-node  --transpile-only --require dotenv/config  --project ./scripts/tsconfig.json -- scripts/whitelist-gql-queries.ts && cp scripts/client-whitelist.json ../avo2-proxy/server/scripts && echo Copied client-whitelist.json to proxy",
    "update-sitemap": "npx ts-node --transpile-only --project ./scripts/tsconfig.json -- scripts/update-sitemap.ts",
    "licenses": "license-checker --csv --out licenses.csv",
    "build-and-analyse": "npm run build && source-map-explorer 'dist/assets/embed-*.js' --no-border-checks",
    "build-and-analyse:embed": "npm run build:embed && source-map-explorer 'dist/assets/*.js' --no-border-checks",
    "analyse": "source-map-explorer 'dist/assets/embed-*.js' --no-border-checks",
    "generate-database-types": "dotenv -e .env.qas graphql-codegen -- --verbose --config codegen.yml",
    "check-bundled-files": "npx depcruise src/embed/main.tsx",
    "graph-bundled-files": "npx depcruise src/embed/main.tsx --include-only \"^src\" --exclude \"(.spec.tsx?|.test.tsx?|node_modules)\" --output-type dot",
    "bump": "bump-package-versions --part=patch --strategy=highest ./package.json"
  },
  "contributors": [
