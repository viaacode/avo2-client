    "react-select": "^3.2.0",
    "react-to-string": "^0.1.1",
    "react-toastify": "^5.3.2",
    "react-zendesk": "^0.1.5",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
