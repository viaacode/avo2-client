    "replace-in-file": "^6.1.0",
    "sort-object-keys": "^1.1.3",
    "ts-jest": "^28.0.5",
    "ts-loader": "^6.0.4",
    "typescript": "^4.7.4"
  },
  "resolutions": {
    "immutable": "3.8.2",
    "oaf-routing": "4.0.1",
    "react": "16.14.0",
    "isomorphic-fetch": "3.0.0",
    "typescript": "4.7.4",
    "graphql": "16.5.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "@typescript-eslint/no-use-before-define": "off"
        }
      }
    ]
  }
}
