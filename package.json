    "history": "^4.10.1",
    "i18next": "^19.5.5",
    "i18next-xhr-backend": "^3.2.2",
    "immer": "^8.0.1",
    "immutable": "3.8.2",
    "lodash-es": "^4.17.21",
    "marked": "^4.0.10",
    "moment": "^2.29.4",
    "node-fetch": "^2.6.7",
    "oaf-react-router": "^2.1.1",
    "oaf-side-effects": "^3.1.0",
