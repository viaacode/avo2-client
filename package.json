{
  "name": "avo2-client",
  "version": "1.24.0",
  "private": true,
  "scripts": {
    "prestart": "npm run env",
    "start": "vite",
    "dev": "dotenv -e .env.qas vite",
    "env": "node ./scripts/env.js && cp env-config.js ./public",
    "build": "cross-env DISABLE_ESLINT_PLUGIN=true node --max_old_space_size=2048 node_modules/react-scripts/bin/react-scripts build",
    "postbuild": "ts-node --transpile-only --project ./scripts/tsconfig.json -- scripts/add-cookiebot-ignore-attributes.ts",
    "unit-tests": "jest --transformIgnorePatterns \"node_modules/(?!(lodash-es))/\"",
    "lint": "eslint \"{src ,test}/**/*.{ts,tsx}\" --ignore-pattern 'src/shared/generated/*.ts' --cache",
    "lint:fix": "eslint \"{src,test}/**/*.{ts,tsx}\" --ignore-pattern 'src/shared/generated/*.ts' --fix",
    "format:fix": "prettier --write \"src/**/*.{ts,tsx,scss,graphql}\"",
    "translations:extract": "ts-node --transpile-only --project ./scripts/tsconfig.json -- scripts/extract-and-replace-translations.ts && npm run format:fix",
    "whitelist-queries": "ts-node  --transpile-only --require dotenv/config  --project ./scripts/tsconfig.json -- scripts/whitelist-gql-queries.ts && cp scripts/client-whitelist.json ../avo2-proxy/server/scripts && echo Copied client-whitelist.json to proxy",
    "update-sitemap": "ts-node --transpile-only --project ./scripts/tsconfig.json -- scripts/update-sitemap.ts",
    "licenses": "license-checker --csv --out licenses.csv",
    "analyse": "npm run build && source-map-explorer 'build/static/js/*.js'",
    "generate-database-types": "dotenv -e .env graphql-codegen -- --verbose --config codegen.yml",
    "bump": "bump-package-versions --part=patch --strategy=highest ./package.json",
    "list-of-changed-translations": "git diff 296b12bc26a4d5c83b8bb35be5f05115ac6356fe b56c8382a7622a13a7f983c682c1094daa514c94 --no-ext-diff --unified=0 --exit-code -a --no-prefix src/shared/translations/nl.json > translation-changes.txt"
  },
  "contributors": [
    "Bert Verhelst <bert.verhelst@studiohyperdrive.be>",
    "Ward Vercruyssen <ward.vercruyssen@studiohyperdrive.be>",
    "Bram Vandenbussche <bram.vandenbussche@studiohyperdrive.be>",
    "Ian Emsens <ian.emsens@studiohypderive.be>",
    "Benjamin Naesen <benjamin.naesen@studiohyperdrive.be>",
    "Andry Charlier <andry.charlier@studiohyperdrive.be>",
    "Enzo Eghermanne <enzo.eghermanne@studiohyperdrive.be>"
