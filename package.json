{
  "name": "avo2-client",
  "version": "1.19.10",
  "private": true,
  "scripts": {
    "preinstall": "",
    "prestart": "npm run env",
    "start": "react-scripts start",
    "start-with-env": "dotenv -e .env.qas npm run start",
    "env": "node ./scripts/env.js && cp env-config.js ./public",
    "build": "cross-env DISABLE_ESLINT_PLUGIN=true node --max_old_space_size=2048 node_modules/react-scripts/bin/react-scripts build",
    "postbuild": "ts-node --transpile-only --project ./scripts/tsconfig.json -- scripts/add-cookiebot-ignore-attributes.ts",
    "unit-tests": "react-scripts test --transformIgnorePatterns \"node_modules/(?!(lodash-es|moment))/\"",
    "lint": "eslint \"{src,test}/**/*.{ts,tsx}\" --cache",
    "lint:fix": "eslint \"{src,test}/**/*.{ts,tsx}\" --fix",
    "format:fix": "prettier --write \"src/**/*.{ts,tsx,scss}\"",
    "extract-translations": "ts-node --transpile-only --project ./scripts/tsconfig.json -- scripts/extract-and-replace-translations.ts && npm run format:fix",
    "whitelist-queries": "ts-node  --transpile-only --require dotenv/config  --project ./scripts/tsconfig.json -- scripts/whitelist-gql-queries.ts && cp scripts/client-whitelist.json ../avo2-proxy/server/scripts",
    "update-sitemap": "ts-node --transpile-only --project ./scripts/tsconfig.json -- scripts/update-sitemap.ts",
    "licenses": "license-checker --csv --out licenses.csv",
    "analyse": "npm run build && source-map-explorer 'build/static/js/*.js'"
  },
  "contributors": [
    "Bert Verhelst <bert.verhelst@studiohyperdrive.be>",
    "Benjamin Naesen <benjamin.naesen@studiohyperdrive.be>",
    "Andry Charlier <andry.charlier@studiohyperdrive.be>",
    "Enzo Eghermanne <enzo.eghermanne@studiohyperdrive.be>",
    "Ian Emsens <ian.emsens@studiohypderive.be>"
  ],
  "dependencies": {
    "@apollo/react-hooks": "^4.0.0",
    "@hookform/resolvers": "^2.9.1",
    "@popperjs/core": "^2.5.4",
    "@react-hook/resize-observer": "^1.2.5",
    "@storybook/addon-actions": "^6.3.12",
    "@viaa/avo2-components": "^1.101.1",
    "@viaa/avo2-types": "2.45.1",
    "apollo-boost": "^0.4.9",
    "apollo-link": "^1.2.14",
    "braft-editor": "^2.3.9",
    "braft-extensions": "^0.1.0",
