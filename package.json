  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "@typescript-eslint/no-use-before-define": "off"
        }
      }
    ]
  }
